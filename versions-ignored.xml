<?xml version="1.0" encoding="UTF-8"?>
<ruleset comparisonMethod="maven"
				 xmlns="https://www.mojohaus.org/VERSIONS/RULE/2.1.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
				 xsi:schemaLocation="https://www.mojohaus.org/VERSIONS/RULE/2.1.0 http://www.mojohaus.org/versions/versions-model/xsd/rule-2.1.0.xsd">
	<ignoreVersions>
		<ignoreVersion type="regex">(?i).*Alpha(?:-?\d+)?</ignoreVersion>
		<ignoreVersion type="regex">(?i).*alpha(?:-?\d+)?</ignoreVersion>
		<ignoreVersion type="regex">(?i).*Beta(?:-?\d+)?</ignoreVersion>
		<ignoreVersion type="regex">(?i).*-B(?:-?\d+)?</ignoreVersion>
		<ignoreVersion type="regex">(?i).*RC(?:-?\d+)?</ignoreVersion>
		<ignoreVersion type="regex">(?i).*CR(?:-?\d+)?</ignoreVersion>
		<ignoreVersion type="regex">(?i).*M(?:-?\d+)?</ignoreVersion>
		<ignoreVersion type="regex">(?i).*SNAP(?:-?\d+)?</ignoreVersion>
		<ignoreVersion type="regex">(?i).*EDR(?:-?\d+)?</ignoreVersion>
		<ignoreVersion type="regex">(?i).*PFD(?:-?\d+)?</ignoreVersion>
		<ignoreVersion type="regex">.*[-_\.](alpha|Alpha|ALPHA|beta|Beta|BETA|EDR|PFD|rc|RC)[-_\.]?.*</ignoreVersion>
	</ignoreVersions>
	<rules>
		<rule groupId="ch.qos.logback" comparisonMethod="numeric">
			<ignoreVersions>
				<ignoreVersion type="regex">2.*</ignoreVersion>
			</ignoreVersions>
		</rule>
		<rule groupId="jakarta.platform" comparisonMethod="numeric">
			<ignoreVersions>
				<ignoreVersion type="regex">9.1.*</ignoreVersion>
				<ignoreVersion type="regex">10.*</ignoreVersion>
			</ignoreVersions>
		</rule>
		<rule groupId="org.apache.deltaspike.*" comparisonMethod="numeric">
			<ignoreVersions>
				<ignoreVersion type="regex">3.*</ignoreVersion>
			</ignoreVersions>
		</rule>
		<rule groupId="org.hibernate.orm" comparisonMethod="numeric">
			<ignoreVersions>
				<ignoreVersion type="regex">6.*</ignoreVersion>
			</ignoreVersions>
		</rule>
		<rule groupId="org.jboss.logging" comparisonMethod="numeric">
			<ignoreVersions>
				<ignoreVersion type="regex">4.*</ignoreVersion>
			</ignoreVersions>
		</rule>
		<rule groupId="org.jboss.weld.*" comparisonMethod="numeric">
			<ignoreVersions>
				<ignoreVersion type="regex">5.*</ignoreVersion>
				<ignoreVersion type="regex">6.*</ignoreVersion>
			</ignoreVersions>
		</rule>
		<rule groupId="org.mockito" comparisonMethod="numeric">
			<ignoreVersions>
				<ignoreVersion type="regex">6.*</ignoreVersion>
			</ignoreVersions>
		</rule>
		<rule groupId="org.testng" comparisonMethod="numeric">
			<ignoreVersions>
				<ignoreVersion type="regex">8.*</ignoreVersion>
			</ignoreVersions>
		</rule>
	</rules>
</ruleset>
